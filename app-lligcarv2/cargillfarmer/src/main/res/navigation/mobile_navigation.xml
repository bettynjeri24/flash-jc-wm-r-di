<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/farmerHomeFragment">

    <!--    /////////////////////////////////////////////////////-->
    <fragment
        android:id="@+id/successfulFragmentWallet"
        android:name="com.ekenya.rnd.cargillfarmer.utils.base.FarmerSuccessfulFragment"
        android:label="fragment_sucessful_wallet"
        tools:layout="@layout/fragment_sucessful_wallet">
        <action
            android:id="@+id/action_successfulFragmentWallet_to_farmerHomeFragment"
            app:destination="@id/farmerHomeFragment" />
    </fragment>
    
    <fragment
        android:id="@+id/commonAuthFragment"
        android:name="com.ekenya.rnd.common.auth.CommonAuthFragment"
        android:label="CommonAuthFragment" />

    <fragment
        android:id="@+id/homeWalletFragmentUssDTest"
        android:name="com.ekenya.rnd.baseapp.ui.offlineussd.OfflineUssdFragment"
        android:label="HomeCargillFragment"
        tools:layout="@layout/fragment_offline_ussd" />

    <!--    /////////////////////////////////////////////////////-->


    <fragment
        android:id="@+id/farmerHomeFragment"
        android:name="com.ekenya.rnd.cargillfarmer.ui.farmerprofile.dashboard.FarmerHomeFragment"
        android:label="FarmerHomeFragment"
        tools:layout="@layout/farmer_home_fragment">
        <action
            android:id="@+id/action_farmerHomeFragment_to_farmerMiniStatementCargillFragment"
            app:destination="@id/farmerMiniStatementCargillFragment" />
        <action
            android:id="@+id/action_farmerHomeFragment_to_transferToTelcoFragment"
            app:destination="@id/transferToTelcoFragment" />
        <action
            android:id="@+id/action_farmerHomeFragment_to_farmerLinkedAccountFragment"
            app:destination="@id/farmerLinkedAccountFragment" />
        <action
            android:id="@+id/action_farmerHomeFragment_to_moreRecentTransaction"
            app:destination="@id/moreRecentTransaction" />
    </fragment>

    <fragment
        android:id="@+id/sendMoneyAccountListFragment"
        android:name="com.ekenya.rnd.cargillfarmer.ui.farmerprofile.fundtransfer.SendMoneyAccountListFragment"
        android:label="SendMoneyAccountList"
        tools:layout="@layout/fragment_send_money_account_list" />
    <fragment
        android:id="@+id/transferToCargillWallet"
        android:name="com.ekenya.rnd.cargillfarmer.ui.farmerprofile.fundtransfer.TransferToCargillWallet"
        android:label="TransferToCargillDIWallet"
        tools:layout="@layout/fragment_transfer_to_cargill_wallet" />
    <fragment
        android:id="@+id/transferToTelcoFragment"
        android:name="com.ekenya.rnd.cargillfarmer.ui.farmerprofile.fundtransfer.TransferToTelcoFragment"

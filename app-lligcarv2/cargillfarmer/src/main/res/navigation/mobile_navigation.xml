    <fragment
        android:id="@+id/farmerAddBeneficiaryFragment"
        android:name="com.ekenya.rnd.cargillfarmer.ui.farmerprofile.manageaccounts.FarmerAddBeneficiaryFragment"
        android:label="AddBeneficiaryAccountFragment"
        tools:layout="@layout/fragment_add_beneficiary_account" />

    <fragment
        android:id="@+id/farmerMiniStatementCargillFragment"
        android:name="com.ekenya.rnd.cargillfarmer.ui.farmerprofile.statements.FarmerMiniStatementCargillFragment"

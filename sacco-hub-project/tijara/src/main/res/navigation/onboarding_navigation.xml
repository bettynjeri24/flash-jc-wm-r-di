<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/onboarding_navigation"
    app:startDestination="@id/onBoardingFragment">
    <fragment
        android:id="@+id/landingPageFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.onboarding.LandingPageFragment"
        android:label=""
        tools:layout="@layout/fragment_landing_page"
        >

        <action
            android:id="@+id/action_landingPageFragment_to_loginSaccoFragment"
            app:destination="@id/loginSaccoFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_landingPageFragment_to_onBoardingFragment"
            app:destination="@id/onBoardingFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>

        <action
            android:id="@+id/action_landingPageFragment_to_countryFragment"
            app:destination="@id/countryFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_landingPageFragment_to_loginPinFragment"
            app:destination="@id/loginPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/onBoardingFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.onboarding.OnBoardingFragment"
        android:label=""
        tools:layout="@layout/fragment_on_boarding" >
        <action
            android:id="@+id/action_onBoardingFragment_to_countryFragment"
            app:destination="@id/countryFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_onBoardingFragment_to_landingPageFragment"
            app:destination="@id/landingPageFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>

    </fragment>
    <fragment
        android:id="@+id/countryFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.onboarding.CountryFragment"
        android:label=""
        tools:layout="@layout/fragment_country">
        <argument android:name="fragmentType"
            app:argType="integer"
            />


        <action
            android:id="@+id/action_countryFragment_to_deviceVerificationFragment"
            app:destination="@id/deviceVerificationFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_countryFragment_to_saccoListFragment"
            app:destination="@id/saccoListFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_countryFragment_to_newSaccoFragment"
            app:destination="@id/newSaccoFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/deviceVerificationFragment"
        tools:layout="@layout/device_verification_fragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.onboarding.DeviceVerificationFragment"
        android:label="" >
        <action
            android:id="@+id/action_deviceVerificationFragment_to_loginSaccoFragment"
            app:destination="@id/loginSaccoFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_deviceVerificationFragment_to_loginPinFragment"
            app:destination="@id/loginPinFragment"
            app:enterAnim="@anim/anim_fade_in"
            app:exitAnim="@anim/anim_fade_out"
            app:popEnterAnim="@anim/anim_fade_in"
            app:popExitAnim="@anim/anim_fade_out"/>
    </fragment>



    <fragment
        android:id="@+id/saccoListFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.SaccoItemFragment"
        android:label=""
        tools:layout="@layout/sacco_items_fragment">

        <action
            android:id="@+id/action_saccoListFragment_to_scannningFragment"
            app:destination="@id/scannningFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/changeFirstPinFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.login.ChangeFirstPinFragment"
        android:label=""
        tools:layout="@layout/change_first_pin_fragment">
        <action
            android:id="@+id/action_changeFirstPinFragment_to_loginPinFragment"
            app:destination="@id/loginPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>

    </fragment>
    <fragment
        android:id="@+id/loginPinFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.login.LoginPinFragment"
        android:label=""
        tools:layout="@layout/fragment_login_pin">
    <action
        android:id="@+id/action_loginPinFragment_to_forgetPinDashboardFragment"
        app:destination="@id/forgetPinDashboardFragment"
        app:enterAnim="@anim/slide_in_right"
        app:exitAnim="@anim/slide_out_left"
        app:popEnterAnim="@anim/slide_in_left"
        app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_loginPinFragment_to_changeFirstPinFragment"
            app:destination="@id/changeFirstPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/loginSaccoFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.onboarding.LoginSaccoFragment"
        android:label=""
        tools:layout="@layout/login_sacco_fragment">
        <action
            android:id="@+id/action_loginSaccoFragment_to_loginPinFragment"
            app:destination="@id/loginPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/scannningFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.ScannningFragment"
        android:label=""
        >
        <action
            android:id="@+id/action_scannningFragment_to_nationalIDFragment"
            app:destination="@id/nationalIDFragment"
            app:enterAnim="@anim/anim_fade_in"
            app:exitAnim="@anim/anim_fade_out"
            app:popEnterAnim="@anim/anim_fade_in"
            app:popExitAnim="@anim/anim_fade_out"/>
        <action
            android:id="@+id/action_scannningFragment_to_passportFragment"
            app:destination="@id/passportFragment" />
    </fragment>
    <fragment
        android:id="@+id/nationalIDFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.NationalIDFragment"
        android:label="" >
        <action
            android:id="@+id/action_nationalIDFragment_to_capturePhotoFragment"
            app:destination="@id/capturePhotoFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <argument
            android:name="fname"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="mname"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="sname"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="id"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="gender"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="dob"
            app:argType="string"
            app:nullable="true"/>
    </fragment>
    <fragment
        android:id="@+id/passportFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.PassportFragment"
        android:label=""
      >
        <argument
            android:name="surname"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="givenname"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="passportno"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="gender"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="dob"
            app:argType="string"
            app:nullable="true"/>
        <action
            android:id="@+id/action_passportFragment_to_passPortPhotoFragment"
            app:destination="@id/passPortPhotoFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/capturePhotoFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.CapturePhotoFragment"
        android:label="" >
        <action
            android:id="@+id/action_capturePhotoFragment_to_selfieFragment"
            app:destination="@id/selfieFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/selfieFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.SelfieFragment"
        android:label=""
        >
        <action
            android:id="@+id/action_selfieFragment_to_moreDetailFragment"
            app:destination="@id/moreDetailFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/moreDetailFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.MoreDetailFragment"
        android:label=""
        tools:layout="@layout/fragment_more_detail" >
        <action
            android:id="@+id/action_moreDetailFragment_to_newPin"
            app:destination="@id/newPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/registarationOtpFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.RegistarationOtpFragment"
        android:label=""
        tools:layout="@layout/device_verification_fragment">
        <action
            android:id="@+id/action_registarationOtpFragment_to_defaultPinFragment"
            app:destination="@id/defaultPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/newPinFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.NewPinFragment"
        android:label="" >
        <action
            android:id="@+id/action_newPinFragment_to_confirmPinFragment"
            app:destination="@id/confirmPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/confirmPinFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.ConfirmPinFragment"
        android:label="" >
        <action
            android:id="@+id/action_confirmPinFragment_to_loginPinFragment"
            app:destination="@id/loginPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/defaultPinFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.DefaultPinFragment"
        android:label="" >
        <action
            android:id="@+id/action_defaultPinFragment_to_newPinFragment"
            app:destination="@id/newPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <argument
            android:name="fragmentType"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/shareFragment"
        android:name="com.ekenya.rnd.tijara.ui.homepage.home.dashboard.shares.ShareFragment"
        android:label="ShareFragment" />
    <fragment
        android:id="@+id/passPortPhotoFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.registration.PassPortPhotoFragment"
        android:label=""
        tools:layout="@layout/passport_photo_fragment">
        <action
            android:id="@+id/action_passPortPhotoFragment_to_selfieFragment"
            app:destination="@id/selfieFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/newSaccoFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.onboarding.NewSaccoFragment"
        android:label="">
        <action
            android:id="@+id/action_newSaccoFragment_to_scannningFragment"
            app:destination="@id/scannningFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/forgetPinDashboardFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.changepassword.forgetPin.ForgetPinDashboardFragment"
        android:label=""
        tools:layout="@layout/fragment_forget_pin_dashboard">
        <action
            android:id="@+id/action_forgetPinDashboardFragment_to_selectIDTypeFragment"
            app:destination="@id/selectIDTypeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_forgetPinDashboardFragment_to_securityQuizFragment"
            app:destination="@id/securityQuizFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/securityQuizFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.changepassword.forgetPin.SecurityQuizFragment"
        android:label=""
        tools:layout="@layout/fragment_security_quiz">
        <action
            android:id="@+id/action_securityQuizFragment_to_defaultPinFragment"
            app:destination="@id/defaultPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>

    <fragment
        android:id="@+id/selectIDTypeFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.changepassword.forgetPin.SelectIDTypeFragment"
        android:label="SelectIDTypeFragment" >
        <action
            android:id="@+id/action_selectIDTypeFragment_to_selfiePinFragment"
            app:destination="@id/selfiePinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/selfiePinFragment"
        android:name="com.ekenya.rnd.tijara.ui.auth.changepassword.forgetPin.SelfiePinFragment"
        android:label="">
        <action
            android:id="@+id/action_selfiePinFragment_to_defaultPinFragment"
            app:destination="@id/defaultPinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>



</navigation>